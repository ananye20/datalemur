    "def is_looping(n):\n",
    "  seen = set()\n",
    "  while True:\n",
    "    if n in seen:\n",
    "      return True\n",
    "    if n == 1:\n",
    "      return False\n",
    "    seen.add(n)\n",
    "    n = sum(int(c)**2 for c in str(n))"
   ]
